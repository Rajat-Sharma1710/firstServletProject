<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" id="WebApp_ID" version="4.0">
  <servlet>
  	<servlet-name>addServlet</servlet-name>
  	<servlet-class>com.servlet.MyName</servlet-class>
  	
  	<init-param>
  		<param-name>name</param-name>
  		<param-value>Rikishi</param-value>
  	</init-param>
  </servlet>
  <servlet-mapping>
  	<servlet-name>addServlet</servlet-name>
  	<url-pattern>/add</url-pattern>
  </servlet-mapping>
  
  <!--  down is the square servlet mapping -->
  
  <servlet>
  	<servlet-name>squareServlet</servlet-name>
  	<servlet-class>com.servlet.SquareOfServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>squareServlet</servlet-name>
  	<url-pattern>/sq</url-pattern>
  </servlet-mapping>
  
  <context-param>
  	<param-name>name</param-name>
  	<param-value>Rajat</param-value>
  </context-param>
</web-app>